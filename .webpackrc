    "/api/base-modules":{
      "target":"http://localhost:8080",
      "changeOrigin":true
    },
    "/api/base-roles":{
      "target":"http://localhost:8080",
      "changeOrigin":true
    },
    "/api/wechatmp":{
      "target":"http://localhost:8080",
      "changeOrigin":true
    },
    "/api/wx-auto-keys":{
            "target":"http://localhost:8080",
            "changeOrigin":true
    },
    "/api/role-permissions":{
      "target":"http://localhost:8080",
      "changeOrigin":true
